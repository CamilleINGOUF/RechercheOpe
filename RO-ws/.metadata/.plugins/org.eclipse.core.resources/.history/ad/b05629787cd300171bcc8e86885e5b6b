		this.fitnessMax = fitnessMax;
		this.perturbationN = 10;
	}
	
	public void setPertubationN(int n)
	{
		this.perturbationN = n;
	}
	
	private  void pertubation()
	{
		
	}

	@Override
	public void run(Solution s) {
		// TODO Auto-generated method stub

	}

}
