
public class RandomSearch extends LocalSearch {

	int numberEvalMax;
	
	double fitnessMax;
	
	public RandomSearch(Problem problem) {
		super(problem);
		
		numberEvalMax = 1000;
		fitnessMax = 30000;
	}
	
	public RandomSearch(Problem problem, int numberEvalMax) {
		super(problem);
		this.numberEvalMax = numberEvalMax;
	}

	@Override
	public void run(Solution s) {

	}

}
