import java.io.File;
import java.io.FileNotFoundException;
import java.util.Scanner;
import java.util.Vector;

public class KBag {
	
	Vector<Integer> profits;
	Vector<Integer> weights;
	
	int c;
	int size;
	float beta;
	
	
	public KBag(String datFile) throws FileNotFoundException {
		
		
		profits = new Vector<Integer>();
		weights = new Vector<Integer>();
		
		for(int i = 0; i < size; i++)
			profits.add(sc.nextInt());

		for(int i = 0; i < size; i++)
			weights.add(sc.nextInt());
		
		c = sc.nextInt();
		
		
		
		sc.close();
	}
	
	private void openDat(String datFile) 
	{
		Scanner sc = new Scanner(new File(datFile));
		size = sc.nextInt();
	}
}
